<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="217.0" prefWidth="431.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Login">
   <!--suppress JavaFxDefaultTag -->
    <children>
      <AnchorPane prefHeight="217.0" prefWidth="431.0">
         <children>
            <Label layoutX="55.0" layoutY="126.0" text="Senha" />
            <PasswordField fx:id="pw" layoutX="123.0" layoutY="121.0" promptText="Senha">
               <effect>
                  <Blend />
               </effect>
            </PasswordField>
            <ComboBox fx:id="combo" layoutX="123.0" layoutY="53.0" prefHeight="26.0" prefWidth="171.0">
               <effect>
                  <Blend />
               </effect>
            </ComboBox>
            <Label layoutX="55.0" layoutY="92.0" text="User" />
            <Label fx:id="Label" layoutX="140.0" layoutY="24.0" prefHeight="16.0" prefWidth="256.0" text="Insira os dados" />
            <TextField fx:id="userName" layoutX="123.0" layoutY="87.0" promptText="Usuario">
               <effect>
                  <Blend />
               </effect>
            </TextField>
            <Label layoutX="56.0" layoutY="58.0" text="Conta" />
            <Button fx:id="login" layoutX="174.0" layoutY="165.0" mnemonicParsing="false" onAction="#login" text="Login" />
            <Label fx:id="invalid_label" layoutX="250.0" layoutY="170.0" />
         </children>
      </AnchorPane>
   </children>
</Pane>
